<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="me.apqx.demo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".CusApp">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".jetpack.JetpackActivity" />
        <activity android:name=".jetpack.navigation.NavActivity"/>
        <activity android:name=".ipc.IpcActivity"/>
        <activity android:name=".tools.ToolsActivity" />
        <activity android:name=".jetpack.databinding.DataBindingActivity" />
        <activity android:name=".test.TestActivity" />
        <activity android:name=".jetpack.livedata.LiveDataActivity" />
        <activity android:name=".jetpack.lifecycle.LifecycleActivity" />
        <activity android:name=".jetpack.workmanager.WorkManagerActivity" />
        <activity android:name=".mvvm.view.MVVMActivity" />
        <activity android:name=".widget.WidgetActivity" />
        <activity android:name=".service.ServiceActivity" />
        <activity android:name=".realm.RealmActivity" />
        <activity android:name=".widget.recycler.RecyclerActivity" />

        <service android:name=".ipc.RemoteService"
            android:process=":ipc"/>
        <service android:name=".service.Service1" />
        <service android:name=".service.Service2" />
    </application>
</manifest>
